<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <class name="domain.Categorie" table="Categorie">
        <id name="idCategorie" column="idCategorie"/>
        <property name="denumire" column="denumire"/>
        <property name="descriere" column="descriere"/>
    </class>

</hibernate-mapping>